  "dependencies": {
    "@types/webpack-env": "^1.16.0",
    "antd": "^4.9.4",
    "axios": "^0.21.1",
    "babel-plugin-react-css-modules": "^5.2.6",
    "classnames": "^2.2.6",
    "gh-pages": "^3.1.0",
    "good-apis": "^0.0.9",
    "immer": "^8.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "use-immer": "^0.4.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fix && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,css,less,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
