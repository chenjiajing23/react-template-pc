{
  "name": "react-template-pc",
  "version": "0.0.4",
  "description": "a react project template",
  "homepage": "https://chenjiajing23.github.io/react-template-pc",
  "private": true,
  "keywords": [],
  "author": "",
  "license": "ISC",
  "sideEffects": [
    "*.css",
    "*.less",
    "./src/config/i18n.ts"
  ],
  "scripts": {
    "dev": "node ./build/dev-server.js",
    "build": "node ./build/prod-build.js",
    "analysis": "npm run build --report",
    "start": "webpack serve --config ./build/webpack.dev.js --progress",
    "mock": "nodemon --watch mock ./node_modules/.bin/good-mock",
    "server": "cd server && yarn && npm run serve",
    "lint": "npm run lint-eslint && npm run lint-stylelint",
    "lint-eslint": "eslint -c .eslintrc.js src --ext .tsx,.ts --color --fix",
    "lint-stylelint": "stylelint --config .stylelintrc.json src/**/*.less --syntax less --fix",
    "gc": "git-cz",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "patch": "standard-version -r patch",
    "minor": "standard-version -r minor",
    "major": "standard-version -r major",
    "pub": "npm run build && npx git push --follow-tags origin master"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-actions": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "address": "^1.1.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.3",
    "compression-webpack-plugin": "^7.1.2",
    "connect-history-api-fallback": "^1.6.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.3",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "good-mock": "^1.2.5",
    "html-webpack-plugin": "^4.5.1",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^4.3.8",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.5",
    "opn": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-less": "^4.0.0",
    "postcss-loader": "^4.2.0",
    "postcss-safe-parser": "^5.0.2",
    "prettier": "^2.2.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-dev-utils": "^11.0.1",
    "redux-devtools-extension": "^2.13.8",
    "standard-version": "^9.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "thread-loader": "^3.0.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.19.0",
    "webpack-build-notifier": "^2.1.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@types/webpack-env": "^1.16.0",
    "antd": "^4.11.2",
    "axios": "^0.21.1",
    "babel-plugin-react-css-modules": "^5.2.6",
    "classnames": "^2.2.6",
    "gh-pages": "^3.1.0",
    "good-apis": "^0.0.10",
    "i18next": "^19.8.5",
    "i18next-browser-languagedetector": "^6.0.1",
    "immer": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-i18next": "^11.8.5",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "use-immer": "^0.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "eslint src --ext .tsx,.ts --color --fix"
    ],
    "./src/**/*.{css,less,scss}": [
      "stylelint --config .stylelintrc.json --fix"
    ],
    ".src/**/*.{tsx,ts,jsx,js,css,less,md}": [
      "prettier --write"
    ]
  }
}